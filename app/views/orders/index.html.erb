<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Origin</th>
      <th>Destination</th>
      <th>Plan</th>
      <th>Insurance</th>
      <th>Status</th>
      <th>Vendor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order.each do |order| %>
      <tr>
        <td><%= order.origin %></td>
        <td><%= order.destination %></td>
        <td><%= order.plan %></td>
        <td><%= order.insurance %></td>
        <td><%= order.status %></td>
        <td><%= order.vendor_id %></td>
        <td><%= link_to 'Show', vendor_order_path(@vendor, order) %></td>
        <td><%= link_to 'Edit', edit_vendor_order_path(@vendor, order) %></td>
        <td><%= link_to 'Destroy', vendor_order_path(@vendor, order), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_vendor_order_path(@vendor) %>
<%= link_to 'Back to Vendor', @vendor %>
